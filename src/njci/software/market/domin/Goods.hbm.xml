<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="njci.software.market.domin.Goods" table="goods">
		<id name="mid" column="mid">
			<generator class="native"></generator>
		</id>
		<property name="name" column="name"></property>
		<property name="price" column="price"></property>
		<property name="type" column="type"></property>
		<property name="num" column="num"></property>
		<property name="pic1" column="pic1"></property>
		<property name="pic2" column="pic2"></property>
		<property name="dates" column="dates"></property>
		<property name="discount" column="discount"></property>
		<property name="total" column="total"></property>
		<property name="operateid" column="operateid"></property>
		<property name="operatertime" column="operatertime"></property>
		<property name="status" column="status"></property>
	</class>
</hibernate-mapping>  