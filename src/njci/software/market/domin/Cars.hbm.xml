<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="njci.software.market.domin.Cars" table="cars">
		<id name="mid" column="mid">
			<generator class="native"></generator>
		</id>
		<property name="uid" column="uid"></property>
		<property name="pid" column="pid"></property>
		<property name="addtime" column="addtime"></property>
		<property name="num" column="num"></property>
		<property name="img" column="img"></property>
		<property name="price" column="price"></property>
		<property name="name" column="name"></property>
		
	</class>
</hibernate-mapping>  